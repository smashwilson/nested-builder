  "author": "Ash Wilson <smashwilson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smashwilson/nested-builder/issues"
  },
  "homepage": "https://github.com/smashwilson/nested-builder#readme",
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  }
}
